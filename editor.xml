<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="1000" />
  </component>
  <component name="LiveEditOptions" chromeUpdateApp="true" chromeUpdateOnChanges="true" nodeUpdateOnChanges="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="bf161b" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="dae31c" />
    </pattern>
    <pattern useCustomColors="true" pattern="\breview\b.*">
      <option name="FOREGROUND" value="2abf1f" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="2122e3" />
    </pattern>
    <filter name="review">
      <pattern index="2" />
    </filter>
    <filter name="fixme">
      <pattern index="1" />
    </filter>
    <filter name="todo">
      <pattern index="0" />
    </filter>
  </component>
</application>