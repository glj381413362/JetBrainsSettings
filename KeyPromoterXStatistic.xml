          <Count>38</Count>
        </Item>
      </Statistic>
      <Statistic Action="Type Declaration">
        <Item>
          <IdeaActionID>GotoTypeDeclaration</IdeaActionID>
          <ShortCut>'⇧⌘B' or '⌃⇧B'</ShortCut>
          <Description>Type Declaration</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update Project...">
        <Item>
          <IdeaActionID>Vcs.UpdateProject</IdeaActionID>
          <ShortCut>'⌘T'</ShortCut>
          <Description>Update Project...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="编码规约扫描">
        <Item>
          <IdeaActionID>AliP3CInspectionAction</IdeaActionID>
          <ShortCut>'⌥⇧⌘J'</ShortCut>
          <Description>编码规约扫描</Description>
          <Count>1</Count>
