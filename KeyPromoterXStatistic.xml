        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'HmallSaApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'⇧⌘N'</ShortCut>
          <Description>Scratch File</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop">
        <Item>
          <IdeaActionID>RunDashboard.Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop</Description>
          <Count>15</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'HmallSaApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'HmallSaApplication'</Description>
          <Count>14</Count>
        </Item>
