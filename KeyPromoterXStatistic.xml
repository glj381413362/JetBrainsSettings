<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit Message History">
        <Item>
          <IdeaActionID>Vcs.ShowMessageHistory</IdeaActionID>
          <ShortCut>'⌃M' or '⌘E'</ShortCut>
          <Description>Commit Message History</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'⌘K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'⌘5'</ShortCut>
          <Description>Debug</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'HmallSaApplication'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'HmallSaApplication'</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>69</Count>
        </Item>
      </Statistic>
      <Statistic Action="Evaluate Expression...">
        <Item>
          <IdeaActionID>EvaluateExpression</IdeaActionID>
          <ShortCut>'⌥F8'</ShortCut>
          <Description>Evaluate Expression...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
